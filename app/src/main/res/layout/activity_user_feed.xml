<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.hoophacks.hoophacks.UserFeed">

    <RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="368dp"
        android:layout_height="495dp"
        android:paddingLeft="16dp"
        android:paddingRight="16dp"
        android:paddingTop="16dp"
        android:paddingBottom="16dp"
        tools:context="nathanial15.parseapp.UserFeed"
        tools:layout_editor_absoluteY="8dp"
        tools:layout_editor_absoluteX="8dp">

        <Button
            android:id="@+id/bLogOut"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="6.18"
            android:text="Log Out"
            android:layout_toRightOf="@+id/bProfile"
            android:layout_toEndOf="@+id/bProfile" />

        <Button
            android:id="@+id/bProfile"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="6.18"
            android:text="Profile" />

    </RelativeLayout>
</android.support.constraint.ConstraintLayout>
